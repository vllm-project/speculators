# Configuration for GPT-OSS-20B with EAGLE3 speculator
# Evaluation on math_reasoning dataset

# Model configuration
BASE_MODEL="RedHatAI/gpt-oss-20b"
SPECULATOR_MODEL="RedHatAI/gpt-oss-20b-speculator.eagle3"
NUM_SPEC_TOKENS=3
METHOD="eagle3"

# Dataset configuration
# Using colon syntax to specify specific file from HuggingFace dataset
DATASET="RedHatAI/speculator_benchmarks:math_reasoning.jsonl"

# vLLM server settings
TENSOR_PARALLEL_SIZE=2
GPU_MEMORY_UTILIZATION=0.8
PORT=8000
HEALTH_CHECK_TIMEOUT=300

# Sampling parameters
TEMPERATURE=0.6
TOP_P=0.95
TOP_K=20

# Output settings
OUTPUT_DIR="eval_results_$(date +%Y%m%d_%H%M%S)"
