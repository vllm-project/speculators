# Configuration for Llama-3.3-70B with EAGLE3 speculator
# Evaluation on math_reasoning dataset

# Model configuration
BASE_MODEL="RedHatAI/Llama-3.3-70B-Instruct-FP8-dynamic"
SPECULATOR_MODEL="RedHatAI/Llama-3.3-70B-Instruct-speculator.eagle3"
NUM_SPEC_TOKENS=3
METHOD="eagle3"

# Dataset configuration
# Using colon syntax to specify specific file from HuggingFace dataset
DATASET="RedHatAI/speculator_benchmarks:math_reasoning.jsonl"

# vLLM server settings
TENSOR_PARALLEL_SIZE=4
MAX_MODEL_LEN=24000
GPU_MEMORY_UTILIZATION=0.85
PORT=8000
HEALTH_CHECK_TIMEOUT=300

# Sampling parameters
TEMPERATURE=0.6
TOP_P=0.95
TOP_K=20

# Output settings
OUTPUT_DIR="eval_results_$(date +%Y%m%d_%H%M%S)"
